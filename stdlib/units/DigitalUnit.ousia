class DigitalUnit extends MetricUnit {

  function kibi = DigitalUnit(1024, "Ki" + symbol)
  function mebi = DigitalUnit(1024^2, "Mi" + symbol)
  function gibi = DigitalUnit(1024^3, "Gi" + symbol)
  function tebi = DigitalUnit(1024^4, "Ti" + symbol)
  function pebi = DigitalUnit(1024^5, "Pi" + symbol)
  function exbi = DigitalUnit(1024^6, "Ei" + symbol)
  function zebi = DigitalUnit(1024^7, "Zi" + symbol)
  function yobi = DigitalUnit(1024^8, "Yi" + symbol)
}
